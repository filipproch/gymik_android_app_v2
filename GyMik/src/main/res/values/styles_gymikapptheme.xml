<resources xmlns:android="http://schemas.android.com/apk/res/android">

  <style name="AutoCompleteTextViewGymikAppTheme" parent="android:Widget.Holo.Light.AutoCompleteTextView">
      <item name="android:dropDownSelector">@drawable/gymikapptheme_list_selector_holo_light</item>
	    <item name="android:background">@drawable/gymikapptheme_edit_text_holo_light</item>
  </style>

  <style name="ButtonGymikAppTheme" parent="android:Widget.Holo.Light.Button">
	  <item name="android:background">@drawable/ab_solid_example</item>
      <item name="android:textColor">@color/button_text_color</item>
  </style>

  <style name="ImageButtonGymikAppTheme" parent="android:Widget.Holo.Light.ImageButton">
	  <item name="android:background">@drawable/gymikapptheme_btn_default_holo_light</item>
  </style>

  <style name="SpinnerGymikAppTheme" parent="android:Widget.Holo.Light.Spinner">
      <item name="android:background">@drawable/gymikapptheme_spinner_background_holo_light</item>
      <item name="android:dropDownSelector">@drawable/gymikapptheme_list_selector_holo_light</item>
  </style>

  <style name="TabGymikAppTheme">
      <item name="android:gravity">center_horizontal</item>
      <item name="android:paddingLeft">16dip</item>
      <item name="android:paddingRight">16dip</item>
      <item name="android:background">@drawable/gymikapptheme_tab_indicator_holo</item>
      <item name="android:layout_width">0dip</item>
      <item name="android:layout_weight">1</item>
      <item name="android:minWidth">80dip</item>
  </style>

  <style name="TabTextGymikAppTheme">
	  <item name="android:textColor">#000000</item>
      <item name="android:textSize">12sp</item>
      <item name="android:textStyle">bold</item>
      <!-- v14 <item name="android:textAllCaps">true</item> -->
      <item name="android:ellipsize">marquee</item>
      <item name="android:maxLines">2</item>
      <item name="android:maxWidth">180dip</item>
  </style>

  <style name="ProgressBarGymikAppTheme" parent="android:Widget.Holo.Light.ProgressBar.Horizontal">
      <item name="android:progressDrawable">@drawable/gymikapptheme_progress_horizontal_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/gymikapptheme_progress_indeterminate_horizontal_holo_light</item>
  </style>

  <style name="SeekBarGymikAppTheme" parent="android:Widget.Holo.Light.SeekBar">
      <item name="android:progressDrawable">@drawable/gymikapptheme_scrubber_progress_horizontal_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/gymikapptheme_scrubber_progress_horizontal_holo_light</item>
      <item name="android:thumb">@drawable/gymikapptheme_scrubber_control_selector_holo_light</item>
  </style>

  <style name="RatingBarGymikAppTheme" parent="android:Widget.Holo.Light.RatingBar">
